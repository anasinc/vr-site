[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "origin"]
	url = https://github.com/anasinc/vr-site
	fetch = +refs/heads/*:refs/remotes/origin/*
[remote "blog"]
	url = https://github.com/anasinc/vr-site
	fetch = +refs/heads/*:refs/remotes/blog/*
[branch "master"]
	remote = blog
	merge = refs/heads/gh-pages
[branch "gh-pages"]
	remote = origin
	merge = refs/heads/gh-pages
[branch "master"]
	remote = origin
	merge = refs/heads/master
